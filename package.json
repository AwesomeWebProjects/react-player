      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --cache --fix"
    ],
