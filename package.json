    "prettier": "prettier --config ./.prettierrc.json --write \"**/*{.js,.jsx,.css}\"",
    "dev": "node ./prebuild.js --env=development && node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.dev.mjs --watch",
    "build": "node ./prebuild.js --env=production && node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.prod.mjs",
    "build:netlify": "node ./prebuild.js --env=production && node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.netlify.mjs",
    "build:debug": "node ./prebuild.js --env=production && node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.prod.mjs --config-debug=true",
    "prepare": "husky install"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "dependencies": {
    "classnames": "2.3.2"
